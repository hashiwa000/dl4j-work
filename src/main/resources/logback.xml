<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{10} %msg%n</pattern>
        </encoder>
    </appender>

    <!-- <logger name="jp.hashiwa.dl4j.sample" level="INFO"/> -->

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>